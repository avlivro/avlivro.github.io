ro","url":"https:\/\/avlivro.aybala.kz\/","logo":{"@type":"ImageObject","inLanguage":"ru-RU","@id":"https:\/\/avlivro.aybala.kz\/#\/schema\/logo\/image\/","url":"https:\/\/avlivro.aybala.kz\/wp-content\/uploads\/2024\/07\/avlivro-logo.webp","contentUrl":"https:\/\/avlivro.aybala.kz\/wp-content\/uploads\/2024\/07\/avlivro-logo.webp","width":512,"height":512,"caption":"Always Visible \u2014 \u0415\u0451 \u0437\u0432\u0430\u043b\u0438 \u0414\u0435\u043b\u0438\u044f"},"image":{"@id":"https:\/\/avlivro.aybala.kz\/#\/schema\/logo\/image\/"}},{"@type":"Person","@id":"https:\/\/avlivro.aybala.kz\/#\/schema\/person\/97087c9fc220756d976ff40934a67adc","name":"Vitaly Ivolginsky (\u0412\u0438\u0442\u0430\u043b\u0438\u0439 \u0418\u0432\u043e\u043b\u0433\u0438\u043d\u0441\u043a\u0438\u0439)","sameAs":["http:\/\/avlivro.aybala.kz"],"url":"https:\/\/avlivro.aybala.kz\/?author=1"}]}},"_links":{"self":[{"href":"https:\/\/avlivro.aybala.kz\/index.php?rest_route=\/wp\/v2\/pages\/181"}],"collection":[{"href":"https:\/\/avlivro.aybala.kz\/index.php?rest_route=\/wp\/v2\/pages"}],"about":[{"href":"https:\/\/avlivro.aybala.kz\/index.php?rest_route=\/wp\/v2\/types\/page"}],"author":[{"embeddable":true,"href":"https:\/\/avlivro.aybala.kz\/index.php?rest_route=\/wp\/v2\/users\/1"}],"replies":[{"embeddable":true,"href":"https:\/\/avlivro.aybala.kz\/index.php?rest_route=%2Fwp%2Fv2%2Fcomments&post=181"}],"version-history":[{"count":3,"href":"https:\/\/avlivro.aybala.kz\/index.php?rest_route=\/wp\/v2\/pages\/181\/revisions"}],"predecessor-version":[{"id":878,"href":"https:\/\/avlivro.aybala.kz\/index.php?rest_route=\/wp\/v2\/pages\/181\/revisions\/878"}],"wp:attachment":[{"href":"https:\/\/avlivro.aybala.kz\/index.php?rest_route=%2Fwp%2Fv2%2Fmedia&parent=181"}],"curies":[{"name":"wp","href":"https:\/\/api.w.org\/{rel}","templated":true}]}}