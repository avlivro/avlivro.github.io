\n\n\n\n<p>\u041f\u0440\u043e\u0440\u0430\u0431\u043e\u0442\u0430\u0432 \u0442\u0435 \u043f\u044f\u0442\u044c \u0434\u043d\u0435\u0439, \u043c\u0438\u0441\u0442\u0435\u0440 \u0422\u0451\u0440\u043b\u043e\u0443 \u0432\u044b\u0448\u0435\u043b \u0438\u0437 \u0437\u0434\u0430\u043d\u0438\u044f, \u0433\u0434\u0435 \u0440\u0430\u0441\u043f\u043e\u043b\u0430\u0433\u0430\u043b\u0441\u044f \u0435\u0433\u043e \u043e\u0444\u0438\u0441, \u0438, \u0443\u0441\u043a\u043e\u0440\u0438\u0432 \u0448\u0430\u0433, \u043d\u0430\u043f\u0440\u0430\u0432\u0438\u043b\u0441\u044f \u043a \u0430\u0432\u0442\u043e\u0431\u0443\u0441\u043d\u043e\u0439 \u043e\u0441\u0442\u0430\u043d\u043e\u0432\u043a\u0435, \u0433\u0434\u0435, \u043a\u0430\u043a \u0432\u0441\u0435\u0433\u0434\u0430, \u0435\u043c\u0443 \u043d\u0443\u0436\u043d\u043e \u0431\u044b\u043b\u043e \u0441\u0435\u0441\u0442\u044c \u0432 \u0430\u0432\u0442\u043e\u0431\u0443\u0441, \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u043f\u0435\u0440\u0435\u0432\u043e\u0437\u0438\u043b \u043b\u044e\u0434\u0435\u0439 \u043f\u043e \u0443\u0436\u0435 \u043e\u043f\u0438\u0441\u0430\u043d\u043d\u043e\u043c\u0443 \u0432\u044b\u0448\u0435 \u043c\u0430\u0440\u0448\u0440\u0443\u0442\u0443. \u041e\u0434\u043d\u0430\u043a\u043e \u0432 \u0442\u043e\u0442 \u043a\u043e\u043d\u043a\u0440\u0435\u0442\u043d\u044b\u0439 \u043c\u043e\u043c\u0435\u043d\u0442 \u043c\u0430\u0448\u0438\u043d\u0430 \u043d\u0435\u043c\u043d\u043e\u0433\u043e \u043e\u043f\u043e\u0437\u0434\u0430\u043b\u0430, \u0438 \u043f\u043e\u044d\u0442\u043e\u043c\u0443 \u0414\u0436\u043e \u0441\u0438\u0434\u0435\u043b \u043d\u0430 \u0441\u043a\u0430\u043c\u0435\u0439\u043a\u0435 \u0441\u043e \u0441\u043a\u0443\u0447\u0430\u044e\u0449\u0438\u043c \u0432\u0438\u0434\u043e\u043c, \u0433\u043b\u044f\u0434\u044f \u043d\u0430 \u0434\u0440\u0443\u0433\u0438\u0445 \u043b\u044e\u0434\u0435\u0439, \u043a\u043e\u0442\u043e\u0440\u044b\u043c \u0442\u043e\u0436\u0435 \u043d\u0443\u0436\u043d\u043e \u0431\u044b\u043b\